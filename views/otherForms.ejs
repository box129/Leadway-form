    <script>
        function notifyParentOfStep(stepIndex) {
            window.parent.postMessage({ type: "STEP_CHANGE", step: stepIndex }, "*");
        }

        document.addEventListener("DOMContentLoaded", () => {
            // Get the current step from the server-side template
            let currentStep = <%= step || 1 %>;
            const forms = document.querySelectorAll(".form");
            
            // Initialize - notify parent of current step
            notifyParentOfStep(currentStep);
            
            // Only show the active form
            forms.forEach((form, index) => {
                if (index + 1 === currentStep) {
                    form.classList.add("active");
                } else {
                    form.classList.remove("active");
                }
            });

            function goToNextStep() {
                if (currentStep < forms.length) {
                    forms[currentStep - 1].classList.remove("active");
                    currentStep++;
                    forms[currentStep - 1].classList.add("active");
                    notifyParentOfStep(currentStep);
                }
            }

            function goToPrevious() {
                if (currentStep > 1) {
                    forms[currentStep - 1].classList.remove("active");
                    currentStep--;
                    forms[currentStep - 1].classList.add("active");
                    notifyParentOfStep(currentStep);
                }
            }

            // Setup event listeners
            document.querySelectorAll("[id^='next']").forEach(button => {
                button?.addEventListener("click", function(e) {
                    // Only prevent default if it's not a submit button in a form
                    if (!button.type || button.type !== "submit") {
                        e.preventDefault();
                    }
                    goToNextStep();
                });
            });

            document.querySelectorAll(".previous0").forEach(button => { 
                button?.addEventListener("click", function(e) {
                    e.preventDefault();
                    goToPrevious();
                });
            });

            // Rest of your event handlers
            // ...


            let drivers = document.getElementById("driver");
            let License = document.getElementById("license");
            let liForm = document.getElementById("license-form");

            driver.addEventListener("click", ()=> {
                drivers.classList.toggle("hidden");
                License.classList.toggle("hidden");
                liForm.classList.toggle("hidden");

            });

            License.addEventListener("click", ()=> {
                drivers.classList.toggle("hidden");
                License.classList.toggle("hidden");
                liForm.classList.toggle("hidden");
            });

            document.getElementById('add-beneficiary').addEventListener('click', function(e) {
                e.preventDefault();

                const container = document.getElementById('beneficiaries-container');
                const lastBeneficiary = container.querySelector('.beneficiary');
                const clone = lastBeneficiary.cloneNode(true); // Deep clone

                // Optional: clear input values in the cloned form
                clone.querySelectorAll('input').forEach(input => input.value = '');
                clone.querySelectorAll('select').forEach(select => select.selectedIndex = 0);

                container.appendChild(clone);


                document.getElementById('add-executive').addEventListener('click', function(e) {
                e.preventDefault();

                const container4 = document.getElementById('executive-container');
                const lastExecutive = container4.querySelector('.executive');
                const clone4 = lastExecutive.cloneNode(true); // Deep clone

                // Optional: clear input values in the cloned form
                clone4.querySelectorAll('input').forEach(input => input.value = '');
                clone4.querySelectorAll('select').forEach(select => select.selectedIndex = 0);

                container4.appendChild(clone4);
                });
            });

            document.getElementById('add-guardian').addEventListener('click', function(e) {
            e.preventDefault();

            const container5 = document.getElementById('guardian-container');
            const lastguardian = container5.querySelector('.guardian');
            const clone5 = lastguardian.cloneNode(true); // Deep clone

            // Optional: clear input values in the cloned form
            clone5.querySelectorAll('input').forEach(input => input.value = '');
            clone5.querySelectorAll('select').forEach(select => select.selectedIndex = 0);

            container5.appendChild(clone5);
            });

            const sections = [
                // { buttonId: "toggleButton0", containerId: "formContainer0"},
                { buttonId: "toggleButton1", containerId: "formContainer1"},
                { buttonId: "toggleButton2", containerId: "formContainer2"},
                { buttonId: "toggleButton3", containerId: "formContainer3"},
                { buttonId: "toggleButton4", containerId: "formContainer4"},
                { buttonId: "toggleButton5", containerId: "formContainer5"},
                { buttonId: "toggleButton6", containerId: "formContainer6"},
                { buttonId: "toggleButton7", containerId: "formContainer7"}
            ];

            sections.forEach(section => {
                const button = document.getElementById(section.buttonId);
                const container = document.getElementById(section.containerId);

                const svgShow = `
                <svg xmlns="http://www.w3.org/2000/svg" width="23" height="23" viewBox="0 0 23 23" fill="none">
                <circle cx="11.5" cy="11.5" r="11.5" fill="#FF6606"/>
                <path d="M11 16L17.0622 10H4.93782L11 16Z" fill="white"/>
                </svg>
                `;

                const svgHide = `
                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="23" viewBox="0 0 24 23" fill="none">
                <circle cx="11.5" cy="11.5" r="11.5" transform="matrix(1 0 0 -1 0.5 23)" fill="#FF6606"/>
                <path d="M11.5 7L17.5622 13H5.43782L11.5 7Z" fill="white"/>
                </svg>`;

                button.addEventListener("click", function(e) {
                    e.preventDefault();
                    container.classList.toggle("hidden");
                    // button.textContent = container.classList.contains("hidden") ? "Show Form" : "Hide Form";

                    button.innerHTML = svgShow;

                    button.addEventListener("click", function (e) {
                        e.preventDefault();
                        container.classList.toggle("hidden");
                        button.innerHTML = container.classList.contains("hidden") ? svgShow : svgHide;
                    });
                });
            });


        // document.addEventListener("DOMContentLoaded", function() {
            
        // });
        });    
            
       
            


    </script>
</body>
</html>